$(function() {
            
    if ($('.navbar-userfrosting').height() > 53) {
            $('.panel-primary').css({"margin-top": "53px"});
        }
    $(window).resize(function() {
        if ($('.navbar-userfrosting').height() > 53) {
            $('.panel-primary').css({"margin-top": "53px"});
        }
        else {
            $('.panel-primary').css({"margin-top": "0px"});
        }
    });

});