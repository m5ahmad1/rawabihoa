.se-pre-con {
	position: fixed;
	left: 0px;
    top: 0px;
	width: 100%;
	height: 100%;
	z-index: 100;
    background: url(../images/loaders/loader1-64x/Preloader_2.gif) center no-repeat #fff;
    background-position: calc(50% + 60px) 50%;
}

@media screen and (max-width: 1024px) {
    .se-pre-con {
        position: fixed;
        left: 0px;
        top: 0px;
        width: 100%;
        height: 100%;
        z-index: 100;
        background: url(../images/loaders/loader1-64x/Preloader_2.gif) center no-repeat #fff;
    }
}

.sidebar-wrapper,.navbar-header,footer {
    z-index: 9999;
}

.footer-width {
    width: calc(100% - 170px);
    left: 170px;
}

@media screen and (max-width: 1024px) {
    .footer-width {
        width: 100%;
        left: 0;
    }
}

.footer-asal {
    text-align: center;
}

body {
    overflow-x:hidden;
}

body.modal-open {
    overflow-y: hidden !important;
}