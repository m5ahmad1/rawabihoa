/*** overriding***/


/*override bootstrap styles*/

.btn-primary {
    background-color: #0e1a35;
    border-color: #2e6da4;
}

.label-primary {
    background-color: #0e1a35;
}

.btn-primary.disabled,
.btn-primary[disabled] {
    background-color: #0e1a35;
    border-color: #2e6da4;
}

.btn-primary:hover,
.btn-primary:focus,
.btn-primary.focus,
.btn-primary:active {
    background-color: #1e3872;
}

.panel-primary {
    border:0;
    width: calc(100% - 200PX);
    border-left: 0;
    float: right;
}

.sepration-line {
    border-bottom: solid;
    margin-bottom: 3px;
}

.finance-type,
.tablesorter td {
    vertical-align: middle !important;
    text-align: center;
}

.tablesorter th {
    text-align: center !important;
}

.modal-unit-table tr , .modal-unit-table th{
    text-align: center;
}

.no-margin{
    margin-top: 0;
    margin-bottom: 0;
}



/*make unit table responsive at small screens*/

@media only screen and (max-width: 466px){
    .tablesorter tr{
        font-size: 15px;
    }
}
@media only screen and (max-width: 444px){
    .tablesorter tr{
        font-size: 12px;
    }
}
@media only screen and (max-width: 370px){
    .tablesorter tr{
        font-size: 10px;
    }
}
@media only screen and (max-width: 768px),
(min-width: 768px) and (max-width: 1024px) {
    .panel-primary {
        width: 100%;
    }
    .save-icon {
        width: 100px;
    }
    .input-group-addon.save-icon {
        width: 40px;
    }
    .inputs {
        width: auto !important;
    }
    .sepration-line {
        display: none;
    }
    /* Force table to not be like tables anymore */
    .tablesorter table,
    .tablesorter thead,
    .tablesorter tbody,
    .tablesorter th,
    .tablesorter td,
    .tablesorter tr {
        display: block;
    }

    /* Hide table headers (but not display: none;, for accessibility) */
    .tablesorter thead tr {
        position: absolute;
        top: -9999px;
        left: -9999px;
    }
    .tablesorter td {
        vertical-align: middle !important;
    }
    .tablesorter tr {
        border: 1px solid #ccc;
    }

    .tablesorter td {
        /* Behave  like a "row" */
        border: none !important;
        border-bottom: 2px solid #eee !important;
        position: relative !important;
        padding-left: 50% !important;
        text-align: left !important;
    }

    .tablesorter td:before {
        /* Now like a table header */
        position: absolute;
        /* Top/left values mimic padding */
        top: 6px;
        left: 6px;
        padding-right: 10px;
        white-space: nowrap;
    }

    .navbar-header {
        float: none;
    }
    .navbar-left,
    .navbar-right {
        float: none !important;
    }
    .navbar-toggle {
        display: block;
    }
    .navbar-collapse {
        border-top: 1px solid transparent;
        box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.1);
    }
    .navbar-fixed-top {
        top: 0;
        border-width: 0 0 1px;
    }
    .navbar-collapse.collapse {
        display: none!important;
    }
    .navbar-nav {
        float: none!important;
        margin-top: 7.5px;
    }
    .navbar-nav>li {
        float: none;
    }
    .navbar-nav>li>a {
        padding-top: 10px;
        padding-bottom: 10px;
    }
    .collapse.in {
        display: block !important;
    }
    .navbar-collapse {
        display: none !important;
    }
    .modal.fade .modal-dialog {

        -webkit-transform: translate(0, 0);
        -ms-transform: translate(0, 0);
        -o-transform: translate(0, 0);
        transform: translate(0, 0);
    }

    .tablesorter td:nth-of-type(1):before {
        content: "User/Info";
    }
    .tablesorter td:nth-of-type(2):before {
        content: "Registered Since";
    }
    .tablesorter td:nth-of-type(3):before {
        content: "Last Sign-in";
    }
    .tablesorter td:nth-of-type(4):before {
        content: "Status/Actions";
    }
    .tablesorter td:nth-of-type(5):before {
        content: "Jan";
    }
    .tablesorter td:nth-of-type(6):before {
        content: "Feb";
    }
    .tablesorter td:nth-of-type(7):before {
        content: "March";
    }
    .tablesorter td:nth-of-type(8):before {
        content: "April";
    }
    .tablesorter td:nth-of-type(9):before {
        content: "May";
    }
    .tablesorter td:nth-of-type(10):before {
        content: "June";
    }
    .tablesorter td:nth-of-type(11):before {
        content: "July";
    }
    .tablesorter td:nth-of-type(12):before {
        content: "Aug";
    }
    .tablesorter td:nth-of-type(13):before {
        content: "Sep";
    }
    .tablesorter td:nth-of-type(14):before {
        content: "Oct";
    }
    .tablesorter td:nth-of-type(15):before {
        content: "Nov";
    }
    .tablesorter td:nth-of-type(16):before {
        content: "Dec";
    }
    .tablesorter td:nth-of-type(17):before {
        content: "Amount due";
    }
    .tablesorter td:nth-of-type(18):before {
        content: "Paid";
    }
    .tablesorter td:nth-of-type(19):before {
        content: "Remaining";
    }
    .tablesorter td:nth-of-type(20):before {
        content: "Save";
    }


    #unitTable td:nth-of-type(1):before {
        content: "Neighborhood" !important;
    }
    #unitTable td:nth-of-type(2):before {
        content: "owner" !important;
    }
    #unitTable td:nth-of-type(3):before {
        content: "Date OF Registration" !important;
    }
    #unitTable td:nth-of-type(4):before {
        content: "Rawabi Code" !important;
    }
    #unitTable td:nth-of-type(5):before {
        content: "Topo Code" !important;
    }
    #unitTable td:nth-of-type(6):before {
        content: "Key Handing" !important;
    }
    #unitTable td:nth-of-type(7):before {
        content: "Actions" !important;
    }
}

